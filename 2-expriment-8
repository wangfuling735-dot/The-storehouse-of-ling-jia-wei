#include<stdio.h>

    int x;
    int y=3;
    int z=2;
    int main ()
{
    printf("输入一个数，我将列出它与1之间的所以素数\n");
    scanf("%d",&x);

    if (x>=2){printf("2\n");}

    while (y<=x){

    while(z<=y){
    if (z==y){printf ("%d\n",y); y=y+1;  z=2; break; }
    else{
    if (y%z==0){y=y+1; z=2; break;}
    else {z=z+1;}
    }

    }

    }

    return 0;


}
